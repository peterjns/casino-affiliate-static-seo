    .faq-item {
        background: rgba(20, 30, 48, 0.5);
        border-radius: 12px;
        margin-bottom: 16px;
        overflow: hidden;
        border: 1px solid rgba(101, 64, 204, 0.2);
        transition: all 0.3s ease;
    }

    .faq-item:hover {
        border-color: rgba(101, 64, 204, 0.4);
