    .faq-toggle {
        width: 24px;
        height: 24px;
        position: relative;
    }

    .faq-toggle::before,
    .faq-toggle::after {
        content: '';
        position: absolute;
        background-color: #6540CC;
        transition: all 0.3s ease;
    }

    .faq-toggle::before {
        width: 2px;
        height: 16px;
        top: 4px;
        left: 11px;
    }

    .faq-toggle::after {
        width: 16px;
        height: 2px;
        top: 11px;
        left: 4px;
    }

    .faq-answer {
        padding: 0 24px;
        overflow: hidden;
        transition: all 0.3s ease;
