        <style>
    .custom-class-toast {
        z-index: 999999999;
        font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
    }
    
    /* Override ALL SweetAlert2 container styles to remove backdrop effects */
    .swal2-container {
        backdrop-filter: none !important;
        -webkit-backdrop-filter: none !important;
        background-color: transparent !important;
    }
    
    /* Ensure no backdrop filters on any SweetAlert2 container variations */
    .swal2-container.swal2-bottom-end,
    .swal2-container.custom-class-toast,
    .swal2-container.swal2-backdrop-show {
        backdrop-filter: none !important;
        -webkit-backdrop-filter: none !important;
        background-color: transparent !important;
    }

    /* Premium Toast Container */
    .swal2-toast {
        background: rgba(26, 32, 44, 0.95) !important;
        backdrop-filter: blur(16px) !important;
        -webkit-backdrop-filter: blur(16px) !important;
        border: 1px solid rgba(101, 64, 204, 0.3) !important;
        border-radius: 12px !important;
        box-shadow: 
            0 10px 30px rgba(0, 0, 0, 0.3),
            0 4px 12px rgba(101, 64, 204, 0.2),
            inset 0 1px 0 rgba(255, 255, 255, 0.1) !important;
        padding: 16px 20px !important;
        min-height: 64px !important;
        width: 380px !important;
        animation: toastSlideIn 0.5s cubic-bezier(0.34, 1.56, 0.64, 1) forwards !important;
        position: relative !important;
        overflow: hidden !important;
    }

    /* Success Toast */
    .swal2-toast.swal2-icon-success {
        border-left: 4px solid #10b981 !important;
        background: rgba(26, 32, 44, 0.95) !important;
    }

    /* Error Toast */
    .swal2-toast.swal2-icon-error {
        border-left: 4px solid #ef4444 !important;
        background: rgba(26, 32, 44, 0.95) !important;
    }

    /* Warning Toast */
    .swal2-toast.swal2-icon-warning {
        border-left: 4px solid #f59e0b !important;
        background: rgba(26, 32, 44, 0.95) !important;
    }

    /* Info Toast */
    .swal2-toast.swal2-icon-info {
        border-left: 4px solid #3b82f6 !important;
        background: rgba(26, 32, 44, 0.95) !important;
    }

    /* Default Toast */
    .swal2-toast:not(.swal2-icon-success):not(.swal2-icon-error):not(.swal2-icon-warning):not(.swal2-icon-info) {
        border-left: 4px solid #6540CC !important;
    }

    /* Toast Content Container */
    .swal2-toast .swal2-html-container {
        display: flex !important;
        align-items: center !important;
        margin: 0 !important;
        padding: 0 !important;
    }

    /* Toast Title */
    .swal2-toast .swal2-title {
        color: #ffffff !important;
        font-size: 14px !important;
        font-weight: 500 !important;
        margin: 0 !important;
        line-height: 1.5 !important;
        text-align: left !important;
    }

    /* Toast Icon Container */
    .swal2-toast .swal2-icon {
        width: 24px !important;
        height: 24px !important;
        margin: 0 12px 0 0 !important;
        border: none !important;
        background: none !important;
        flex-shrink: 0 !important;
        animation: iconBounce 0.6s cubic-bezier(0.68, -0.55, 0.265, 1.55) !important;
    }

    /* Success Icon */
    .swal2-toast .swal2-icon.swal2-success {
        color: #10b981 !important;
    }

    .swal2-toast .swal2-icon.swal2-success .swal2-success-line {
        background-color: #10b981 !important;
    }

    .swal2-toast .swal2-icon.swal2-success::after {
        content: '✓' !important;
        font-size: 16px !important;
        font-weight: bold !important;
        color: #10b981 !important;
        display: flex !important;
        align-items: center !important;
        justify-content: center !important;
        width: 24px !important;
        height: 24px !important;
        border-radius: 50% !important;
        background: rgba(16, 185, 129, 0.15) !important;
    }

    /* Error Icon */
    .swal2-toast .swal2-icon.swal2-error {
        color: #ef4444 !important;
    }

    .swal2-toast .swal2-icon.swal2-error::after {
        content: '✕' !important;
        font-size: 14px !important;
        font-weight: bold !important;
        color: #ef4444 !important;
        display: flex !important;
        align-items: center !important;
        justify-content: center !important;
        width: 24px !important;
        height: 24px !important;
        border-radius: 50% !important;
        background: rgba(239, 68, 68, 0.15) !important;
    }

    /* Warning Icon */
    .swal2-toast .swal2-icon.swal2-warning {
        color: #f59e0b !important;
    }

    .swal2-toast .swal2-icon.swal2-warning::after {
        content: '⚠' !important;
        font-size: 14px !important;
        color: #f59e0b !important;
        display: flex !important;
        align-items: center !important;
        justify-content: center !important;
        width: 24px !important;
        height: 24px !important;
        border-radius: 50% !important;
        background: rgba(245, 158, 11, 0.15) !important;
    }

    /* Info Icon */
    .swal2-toast .swal2-icon.swal2-info {
        color: #3b82f6 !important;
    }

    .swal2-toast .swal2-icon.swal2-info::after {
        content: 'ℹ' !important;
        font-size: 14px !important;
        color: #3b82f6 !important;
        display: flex !important;
        align-items: center !important;
        justify-content: center !important;
        width: 24px !important;
        height: 24px !important;
        border-radius: 50% !important;
        background: rgba(59, 130, 246, 0.15) !important;
    }

    /* Hide default SweetAlert2 icon styling */
    .swal2-toast .swal2-icon::before,
    .swal2-toast .swal2-success-ring,
    .swal2-toast .swal2-error-ring,
    .swal2-toast .swal2-success-line,
    .swal2-toast .swal2-error-line,
    .swal2-toast .swal2-x-mark {
        display: none !important;
    }

    /* Toast Container Flex Layout */
    .swal2-popup.swal2-toast.swal2-icon-error.swal2-hide {
        display: flex !important;
        align-items: center !important;
    }

    /* Close Button */
    .swal2-toast .swal2-close {
        position: absolute !important;
        top: 6px !important;
        right: 6px !important;
        width: 20px !important;
        height: 20px !important;
        background: rgba(255, 255, 255, 0.1) !important;
        border-radius: 50% !important;
        color: rgba(255, 255, 255, 0.6) !important;
        font-size: 12px !important;
        line-height: 20px !important;
        transition: all 0.2s ease !important;
        opacity: 0 !important;
    }

    .swal2-toast:hover .swal2-close {
        opacity: 1 !important;
    }

    .swal2-toast .swal2-close:hover {
        background: rgba(255, 255, 255, 0.2) !important;
        color: #ffffff !important;
        transform: scale(1.1) !important;
    }

    /* Progress Bar */
    .swal2-toast .swal2-timer-progress-bar {
        background: rgba(101, 64, 204, 0.6) !important;
        height: 2px !important;
        border-radius: 0 0 12px 12px !important;
    }

    /* Success Progress Bar */
    .swal2-toast.swal2-icon-success .swal2-timer-progress-bar {
        background: rgba(16, 185, 129, 0.6) !important;
    }

    /* Error Progress Bar */
    .swal2-toast.swal2-icon-error .swal2-timer-progress-bar {
        background: rgba(239, 68, 68, 0.6) !important;
    }

    /* Warning Progress Bar */
    .swal2-toast.swal2-icon-warning .swal2-timer-progress-bar {
        background: rgba(245, 158, 11, 0.6) !important;
    }

    /* Info Progress Bar */
    .swal2-toast.swal2-icon-info .swal2-timer-progress-bar {
        background: rgba(59, 130, 246, 0.6) !important;
    }

    /* Slide-in Animation */
    @keyframes toastSlideIn {
        0% {
            transform: translateX(100%);
            opacity: 0;
        }
        100% {
            transform: translateX(0);
            opacity: 1;
        }
    }

    /* Slide-out Animation */
    @keyframes toastSlideOut {
        0% {
            opacity: 1;
            transform: translateX(0);
        }
        100% {
            opacity: 0;
            transform: translateX(100%);
        }
    }

    /* Icon Bounce */
    @keyframes iconBounce {
        0% {
            transform: scale(0);
        }
        50% {
            transform: scale(1.2);
        }
        100% {
            transform: scale(1);
